generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Answers {
  id Int @default(autoincrement()) @id
  Assessment String?
  Campaign String? 
  Item_ID String? 
  Item_Title String? 
  Item_Type String? 
  Statement_Labels String? 
  User_Email String? 
  User_ID String? 
  User_Input String?
  User_Labels String?
  User_Name String? 
  Verification_Status String? 
}
